{
  "name": "demo_credit",
  "version": "1.0.0",
  "description": "This is the API and backend server for the Demo Credit mobile application.",
  "main": "./dist/src/server.js",
  "scripts": {
    "migrate": "knex migrate:latest --knexfile ./src/config/knex.config.ts",
    "rollback": "knex migrate:rollback --knexfile ./src/config/knex.config.ts",
    "seed": "knex seed:run --knexfile ./src/config/knex.config.ts",
    "start": "node -r ./tsconfig-paths-bootstrap.js ./dist/src/server",
    "dev": "nodemon ./src/server.ts",
    "dev-prod": "set NODE_ENV=production&& nodemon ./src/server.ts",
    "build": "tsc -p .",
    "test": "jest --detectOpenHandles",
    "start:prod": "pm2 start --env production"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/khomohzie/demo-credit.git"
  },
  "keywords": [
    "mobile",
    "express",
    "typescript",
    "mysql",
    "knex",
    "api",
    "server",
    "backend"
  ],
  "author": "Daniel Komolafe",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/khomohzie/demo-credit/issues"
  },
  "homepage": "https://github.com/khomohzie/demo-credit#readme",
  "dependencies": {
    "axios": "^1.2.2",
    "bcrypt": "^5.1.0",
    "chalk": "4.1.2",
    "cloudinary": "^1.33.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-formidable-typescript": "^1.0.2",
    "farmhash": "^3.2.2",
    "helmet": "^6.0.1",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^9.0.0",
    "jwt-promisify": "^1.1.6",
    "knex": "^2.3.0",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.8.0",
    "objection": "^3.0.1",
    "paystack": "^2.0.1",
    "paystack-api": "^2.0.6",
    "peer": "^0.6.1",
    "rand-token": "^1.0.1",
    "redis": "^4.5.1",
    "scrawny": "^1.0.0",
    "toobusy-js": "^0.5.1",
    "uuid": "^9.0.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/formidable": "^2.0.5",
    "@types/hpp": "^0.2.2",
    "@types/jest": "^29.2.5",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.11.18",
    "@types/nodemailer": "^6.4.7",
    "@types/paystack": "^2.0.1",
    "@types/supertest": "^2.0.12",
    "@types/toobusy-js": "^0.5.2",
    "@types/uuid": "^9.0.0",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4"
  }
}